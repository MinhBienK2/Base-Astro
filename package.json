{
  "name": "prj-else-global-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx,astro}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,astro}\"",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "astro": "^5.5.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-plugin-astro": "^1.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,astro}": [
      "yarn run lint:fix",
      "yarn run format",
      "git add ."
    ]
  }
